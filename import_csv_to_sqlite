import csv, sqlite3

con = sqlite3.connect("db.sqlite3")
cur = con.cursor()

with open('salary_predicted.csv','rb') as fin:
    dr = csv.DictReader(fin)
    to_db = [(i[0], i['workedYears'], i['salaryBrutto']) for i in dr]

cur.executemany("INSERT INTO t (id, worked_years, salary) VALUES (?, ?);", to_db)
con.commit()
con.close()
